 SLATER 
  ORBITALS {
  CUTOFF_MO
    MAGNIFY 1
    NMO 6 
    ORBFILE qw.orb
    INCLUDE qw.basis
    CENTERS { USEGLOBAL } 
  }

  DETWT { 1.0 1.0 } 
  STATES {
  #DET 1
     2 1 
  #DET 2
     2 3 
   }

   OPTIMIZE_MO
   OPTIMIZE_DATA { 
   #DET 1
     DET { 
      #Orbitals that can mix
      ORB_GROUP { 1 2 4 }
     }
   #DET 2
     DET { 
      ORB_GROUP { 2 1 3 6 }
      #Initial parameter(s) for above orb_group
      PARAMETERS { 0.1 -0.1 0.1 0 0 0 -0.1 0.2 -0.2 0 0 0 }
     }
   }
